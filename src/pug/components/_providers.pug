// SECTION providers :: START

section#h-providers.h-providers
  .c-grid
    .h-providers__wrapper

      h2.h-providers__title!= "Our Liquidity Providers"

      .h-providers__row

        mixin swiperContent()
          .swiper-slide
            img(src="./img/img-partners-1.png" srcset="./img/img-partners-1@2x.png 2x" alt="" title="")
          .swiper-slide
            img(src="./img/img-partners-2.png" srcset="./img/img-partners-2@2x.png 2x" alt="" title="")
          .swiper-slide
            img(src="./img/img-partners-3.png" srcset="./img/img-partners-3@2x.png 2x" alt="" title="")
          .swiper-slide
            img(src="./img/img-partners-4.png" srcset="./img/img-partners-4@2x.png 2x" alt="" title="")
          .swiper-slide
            img(src="./img/img-partners-5.png" srcset="./img/img-partners-5@2x.png 2x" alt="" title="")
          .swiper-slide
            img(src="./img/img-partners-6.png" srcset="./img/img-partners-6@2x.png 2x" alt="" title="")
          .swiper-slide
            img(src="./img/img-partners-7.png" srcset="./img/img-partners-7@2x.png 2x" alt="" title="")
          .swiper-slide
            img(src="./img/img-partners-8.png" srcset="./img/img-partners-8@2x.png 2x" alt="" title="")

        +swiper('swiper-container-providers', 'swiperContent', false, false, false)

// SECTION providers :: END